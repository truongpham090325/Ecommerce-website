.startbar.d-print-none
  // start brand
  .brand
    a.logo(href=`/${pathAdmin}/dashboard`)
      span
        img.logo-sm(
          alt="logo-small" 
          src="/admin/assets/images/logo-sm.png"
          )
      span
        img.logo-lg.logo-light(
          alt="logo-large" 
          src="/admin/assets/images/logo-light.png"
          )
        img.logo-lg.logo-dark(
          alt="logo-large" 
          src="/admin/assets/images/logo-dark.png"
          )
  // end brand
  // start startbar-menu
  .startbar-menu
    #startbarCollapse.startbar-collapse(data-simplebar="")
      .d-flex.align-items-start.flex-column.w-100
        // Navigation
        ul.navbar-nav.mb-auto.w-100
          li.nav-item
            a.nav-link(
              href=`/${pathAdmin}/dashboard`
              )
              i.iconoir-home-simple.menu-icon
              span Tổng quan
          // end nav-item
          li.nav-item
            a.nav-link(
              aria-controls="sidebarArticle" 
              aria-expanded="false" 
              role="button" 
              data-bs-toggle="collapse" 
              href="#sidebarArticle"
              )
              i.iconoir-journal-page.menu-icon
              span Bài viết
            #sidebarArticle.collapse
              ul.nav.flex-column
                li.nav-item
                  a.nav-link(href=`/${pathAdmin}/article/create`) Tạo bài viết
                // end nav-item
                li.nav-item
                  a.nav-link(href=`/${pathAdmin}/article/list`) Danh sách bài viết
                // end nav-item
                li.nav-item
                  a.nav-link(href=`/${pathAdmin}/article/category`) Danh mục bài viết
                // end nav-item
              // end nav
            // end startbarApplications
          // end nav-item
          li.nav-item
            a.nav-link(
              aria-controls="sidebarRole" 
              aria-expanded="false" 
              role="button" 
              data-bs-toggle="collapse" 
              href="#sidebarRole"
              )
              i.iconoir-user-badge-check.menu-icon
              span Nhóm quyền
            #sidebarRole.collapse
              ul.nav.flex-column
                li.nav-item
                  a.nav-link(href=`/${pathAdmin}/role/create`) Tạo nhóm quyền
                // end nav-item
                li.nav-item
                  a.nav-link(href=`/${pathAdmin}/role/list`) Danh sách nhóm quyền
                // end nav-item
          li.nav-item
            a.nav-link(
              aria-controls="sidebarAccountAdmin" 
              aria-expanded="false" 
              role="button" 
              data-bs-toggle="collapse" 
              href="#sidebarAccountAdmin"
              )
              i.iconoir-user-crown.menu-icon
              span Tài khoản quản trị
            #sidebarAccountAdmin.collapse
              ul.nav.flex-column
                li.nav-item
                  a.nav-link(href=`/${pathAdmin}/account-admin/create`) Tạo tài khoản
                // end nav-item
                li.nav-item
                  a.nav-link(href=`/${pathAdmin}/account-admin/list`) Danh sách tài khoản
                // end nav-item
          li.nav-item
            a.nav-link(
              href=`/${pathAdmin}/file-manager/iframe`
              )
              i.iconoir-folder-settings.menu-icon
              span Quản lý file
          // end nav-item
    // end startbar-collapse
  // end startbar-menu
// end startbar
.startbar-overlay.d-print-none